{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "image": "ghcr.io/s0up4200/autobrr-distroless:v1.73.0",
      "name": "autobrr",
      "isMain": true,
      "internalPort": 7474,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "user": "1000:1000",
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/autobrr",
          "containerPath": "/config"
        }
      ]
    }
  ]
}
