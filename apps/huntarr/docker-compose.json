{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "image": "ghcr.io/plexguide/huntarr:8.2.23",
      "name": "huntarr",
      "isMain": true,
      "internalPort": 9705,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/config"
        }
      ]
    }
  ]
}
