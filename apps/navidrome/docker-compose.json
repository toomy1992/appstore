{
    "schemaVersion": 2,
    "services": [
        {
            "name": "navidrome",
            "image": "deluan/navidrome:0.60.2",
            "environment": [
                {
                    "key": "TZ",
                    "value": "${TZ}"
                },
                {
                    "key": "PUID",
                    "value": "1000"
                },
                {
                    "key": "PGID",
                    "value": "1000"
                },
                {
                    "key": "ND_LOGLEVEL",
                    "value": "${NAVIDROME_LOGLEVEL:-info}"
                },
                {
                    "key": "ND_ENABLEARTWORKPRECACHE",
                    "value": "${NAVIDROME_ENABLEARTWORKPRECACHE:-true}"
                },
                {
                    "key": "ND_ENABLEUSEREDITING",
                    "value": "${NAVIDROME_ENABLEUSEREDITING:-true}"
                },
                {
                    "key": "ND_ENABLEDOWNLOADS",
                    "value": "${NAVIDROME_ENABLEDOWNLOADS:-true}"
                },
                {
                    "key": "ND_ENABLESHARING",
                    "value": "${NAVIDROME_ENABLESHARING:-false}"
                },
                {
                    "key": "ND_ENABLETRANSCODINGCONFIG",
                    "value": "${NAVIDROME_ENABLETRANSCODINGCONFIG:-false}"
                },
                {
                    "key": "ND_SESSIONTIMEOUT",
                    "value": "${NAVIDROME_SESSIONTIMEOUT:-48h}"
                },
                {
                    "key": "ND_BASEURL",
                    "value": "${NAVIDROME_BASEURL}"
                },
                {
                    "key": "ND_ENABLEFAVOURITES",
                    "value": "${NAVIDROME_ENABLEFAVOURITES:-true}"
                },
                {
                    "key": "ND_ENABLENOWPLAYING",
                    "value": "${NAVIDROME_ENABLENOWPLAYING:-true}"
                },
                {
                    "key": "ND_ENABLESCROBBLEHISTORY",
                    "value": "${NAVIDROME_ENABLESCROBBLEHISTORY:-true}"
                },
                {
                    "key": "ND_ENABLESTARRATING",
                    "value": "${NAVIDROME_ENABLESTARRATING:-true}"
                },
                {
                    "key": "ND_ENABLEINSIGHTSCOLLECTOR",
                    "value": "${NAVIDROME_ENABLEINSIGHTSCOLLECTOR:-true}"
                },
                {
                    "key": "ND_LASTFM_ENABLED",
                    "value": "${NAVIDROME_LASTFM_ENABLED:-true}"
                },
                {
                    "key": "ND_SPOTIFY_ENABLED",
                    "value": "${NAVIDROME_SPOTIFY_ENABLED:-false}"
                },
                {
                    "key": "ND_SPOTIFY_ID",
                    "value": "${NAVIDROME_SPOTIFY_ID}"
                },
                {
                    "key": "ND_SPOTIFY_SECRET",
                    "value": "${NAVIDROME_SPOTIFY_SECRET}"
                },
                {
                    "key": "ND_DEEZER_ENABLED",
                    "value": "${NAVIDROME_DEEZER_ENABLED:-true}"
                },
                {
                    "key": "ND_LISTENBRAINZ_ENABLED",
                    "value": "${NAVIDROME_LISTENBRAINZ_ENABLED:-true}"
                },
                {
                    "key": "ND_ENABLEREPLAYGAIN",
                    "value": "${NAVIDROME_ENABLEREPLAYGAIN:-true}"
                },
                {
                    "key": "ND_TRANSCODINGCACHESIZE",
                    "value": "${NAVIDROME_TRANSCODINGCACHESIZE:-100MB}"
                },
                {
                    "key": "ND_IMAGECACHESIZE",
                    "value": "${NAVIDROME_IMAGECACHESIZE:-100MB}"
                }
            ],
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data",
                    "containerPath": "/data",
                    "readOnly": false,
                    "shared": false,
                    "private": false
                },
                {
                    "hostPath": "${APP_DATA_DIR}/music",
                    "containerPath": "/music",
                    "readOnly": false,
                    "shared": false,
                    "private": false
                }
            ]
        }
    ]
}